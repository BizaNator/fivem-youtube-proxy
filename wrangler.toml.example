name = "youtube-proxy"
main = "src/index.js"
compatibility_date = "2024-01-01"

# Get these from your Cloudflare dashboard:
# dash.cloudflare.com → Your Domain → Overview → right sidebar
account_id = "YOUR_ACCOUNT_ID_HERE"

routes = [
  { pattern = "yt.yourdomain.com/*", zone_id = "YOUR_ZONE_ID_HERE" }
]
